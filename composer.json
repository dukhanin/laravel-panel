{
    "name": "dukhanin/laravel-panel",
    "description": "Laravel 5 panel constructor",
    "keywords": ["laravel", "panel", "form", "admin", "lists", "list"],
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Anton Dukhanin",
            "email": "anton.dukhanin@gmail.com"
        }
    ],
    "require": {
        "php": "~7.0",
        "intervention/image": "^2.3",
        "dukhanin/laravel-support": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dukhanin\\Panel\\Providers\\PanelServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "laravel/laravel": "5.4"
    },
    "autoload": {
        "psr-4": {
            "Dukhanin\\Panel\\": "src/",
            "Dukhanin\\Panel\\Tests\\": "tests/"
        },
        "files": [
            "src/helpers.php"
        ]
    }
}
